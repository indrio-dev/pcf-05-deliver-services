  "query_examples": {
    "query_1": {
      "question": "When are tomatoes available in South Florida?",
      "cypher": "MATCH (s:State {id: 'state:FL'})-[:HAS_ZONE]->(z:Zone {id: 'zone:10'})-[:PREDICTS]->(hw:HarvestWindow)-[:IS_FOR]->(p:Product {name: 'Tomato'}) RETURN hw.months",
      "traversal": [
        "state:FL",
        "zone:10",
        "harvest:fl_tomato_zone10"
      ],
      "result": {
        "months": [10, 11, 12, 1, 2, 3, 4, 5, 6],
        "peakMonths": [12, 1, 2, 3],
        "description": "October through June with peak December-March"
      },
      "confidence": 0.90,
      "sources": ["FL Dept of Agriculture", "Zone climate data"]
    },
    "query_2": {
      "question": "What farms in Zone 10 grow heritage tomatoes?",
      "cypher": "MATCH (z:Zone {id: 'zone:10'})<-[:LOCATED_IN]-(f:Farm)-[:GROWS]->(p:Product {name: 'Tomato'})<-[:IS_VARIETY_OF]-(c:Cultivar) WHERE c.heritageIntent IN ['true_heritage', 'heirloom_quality'] RETURN f, c",
      "traversal": [
        "zone:10",
        "farm:cypress_creek",
        "cultivar:cherokee_purple",
        "cultivar:brandywine"
      ],
      "result": {
        "farms": [
          {
            "farmName": "Cypress Creek Farms and Ranch",
            "zone": "10a",
            "cultivars": ["Cherokee Purple", "Brandywine"]
          }
        ]
      },
      "confidence": 0.85,
      "sources": ["LocalHarvest farm data", "CSAware product listings"]
    },
    "query_3": {
      "question": "When should I plant Cherokee Purple in Homestead, FL?",
      "cypher": "MATCH (r:Region {id: 'region:homestead_redland'})-[:IN_ZONE]->(z:Zone)-[:HAS_PLANTING_WINDOW]->(pw:PlantingWindow)-[:FOR_CULTIVAR]->(c:Cultivar {name: 'Cherokee Purple'}) RETURN pw.plantingMonths, pw.windows",
      "traversal": [
        "region:homestead_redland",
        "zone:10",
        "planting:fl_tomato_zone10",
        "cultivar:cherokee_purple"
      ],
      "result": {
        "plantingMonths": [8, 9],
        "windows": ["August-September (fall planting)"],
        "expectedHarvest": "October-December (80 days after planting)",
        "gddRequired": 2800,
        "reasoning": "Zone 10b accumulates ~22 GDD/day = 75 days to harvest"
      },
      "confidence": 0.80,
      "sources": ["FL planting guide", "GDD model", "Cultivar data"]
    },
    "query_4": {
      "question": "Validate Cypress Creek's tomato timing against state data",
      "cypher": "MATCH (f:Farm {id: 'farm:cypress_creek'})-[:REPORTS_HARVEST]->(fh:HarvestWindow)-[:VALIDATES]->(zh:HarvestWindow)-[:VALIDATES]->(sh:HarvestWindow) WHERE sh.geography = 'FL (statewide aggregate)' RETURN fh.months, zh.months, sh.months",
      "traversal": [
        "farm:cypress_creek",
        "harvest:cypress_creek_tomato",
        "harvest:fl_tomato_zone10",
        "harvest:fl_tomato_state"
      ],
      "result": {
        "farmTiming": {
          "months": [11, 12, 1, 2, 3, 4],
          "source": "Cypress Creek CSAware"
        },
        "zoneTiming": {
          "months": [10, 11, 12, 1, 2, 3, 4, 5, 6],
          "source": "FL Zone 10 aggregate"
        },
        "stateTiming": {
          "months": [10, 11, 12, 1, 2, 3, 4, 5, 6],
          "source": "FL Dept of Agriculture"
        },
        "validation": {
          "status": "PASS",
          "reasoning": "Farm timing (Nov-Apr) is subset of zone window (Oct-Jun)",
          "confidence": "high",
          "notes": "Farm's narrower window may reflect choice to focus on peak quality months"
        }
      },
      "confidence": 0.85,
      "sources": ["CSAware data", "State calendar", "Zone mapping"]
    },
    "query_5": {
      "question": "Why can Zone 10 grow tomatoes year-round but Zone 8 cannot?",
      "cypher": "MATCH (z8:Zone {id: 'zone:8'}), (z10:Zone {id: 'zone:10'}) RETURN z8.frostRisk, z8.growingSeason, z10.frostRisk, z10.growingSeason",
      "traversal": [
        "zone:8",
        "zone:10"
      ],
      "result": {
        "zone8": {
          "tempRange": "10-20°F winter minimum",
          "frostRisk": "Hard freezes occur",
          "growingSeason": "March-November (9 months)",
          "tomatoWindow": "May-July (spring), October-November (fall)",
          "limitation": "Winter too cold - tomatoes killed by frost"
        },
        "zone10": {
          "tempRange": "30-40°F winter minimum",
          "frostRisk": "Extremely rare or none",
          "growingSeason": "Year-round",
          "tomatoWindow": "October-June (9 months)",
          "advantage": "Frost-free winter enables continuous production"
        },
        "reasoning": "Tomatoes are frost-sensitive. Zone 8 experiences hard freezes November-March. Zone 10 rarely drops below 30°F, allowing winter production when eastern US markets demand Florida tomatoes."
      },
      "confidence": 0.95,
      "sources": ["USDA zone definitions", "Crop physiology", "State harvest calendars"]
    }
  },
