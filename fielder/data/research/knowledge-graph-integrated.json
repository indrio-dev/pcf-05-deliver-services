  "queryEngine": {
    "description": "Sample queries demonstrating how the knowledge graph enables intelligent harvest predictions with validation",

    "queries": [
      {
        "id": "query_1",
        "question": "When should I plant Cherokee Purple tomatoes in South Florida (Zone 10)?",
        "traversal": [
          "cultivar:cherokee_purple",
          "zone:10",
          "plantingWindow"
        ],
        "result": {
          "answer": "Two planting windows available",
          "windows": [
            {
              "season": "Fall planting (RECOMMENDED)",
              "months": "August-September",
              "transplantDate": "Start seeds 6 weeks before (June-July)",
              "confidence": 0.95,
              "sources": ["Burpee", "Mary's Heirloom", "UF/IFAS"],
              "validation": "3-way consensus"
            },
            {
              "season": "Winter planting",
              "months": "January-March",
              "transplantDate": "Start seeds 6 weeks before (Nov-Jan)",
              "confidence": 0.90,
              "sources": ["Burpee", "UF/IFAS"],
              "validation": "2-way consensus"
            }
          ],
          "avoidMonths": "May-July (excessive heat reduces fruit set)"
        }
      },
      {
        "id": "query_2",
        "question": "When will I harvest if I plant Cherokee Purple in Zone 10 on August 15?",
