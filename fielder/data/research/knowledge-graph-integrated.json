{
  "metadata": {
    "name": "Fielder Agricultural Intelligence Knowledge Graph",
    "version": "1.0.0",
    "created": "2025-12-20",
    "description": "Integrated knowledge graph combining state calendars, extension research, seed company data, and farm-level observations for harvest timing predictions",
    "dataSourceCount": 4,
    "totalEntities": 156,
    "totalRelationships": 412,
    "coverageStates": ["FL"],
    "coverageZones": ["8a", "8b", "9a", "9b", "10a", "10b", "11a"]
  },

  "schema": {
    "entityTypes": {
      "State": {
        "description": "US state with agricultural production",
        "attributes": ["name", "abbreviation", "zones", "climaticRegions"]
      },
      "Zone": {
        "description": "USDA Plant Hardiness Zone - THE universal primary key",
        "attributes": ["zoneId", "temperatureRange", "avgGDD", "frostFree", "growingSeason"]
      },
      "Region": {
        "description": "Geographic/climatic sub-region within state",
        "attributes": ["name", "zones", "typicalSoil", "microclimate"]
      },
      "Farm": {
        "description": "Individual farm with products and timing data",
        "attributes": ["name", "location", "zone", "practices", "products", "seasonalWindow"]
      },
      "Product": {
        "description": "Agricultural product type (tomato, orange, etc.)",
        "attributes": ["name", "category", "subcategory", "isClimacteric"]
      },
      "Cultivar": {
        "description": "Specific variety of a product",
        "attributes": ["name", "product", "heritageIntent", "daysToMaturity", "gddRequirement", "qualityAttributes"]
      },
      "PlantingWindow": {
        "description": "Zone-specific planting timing",
        "attributes": ["zone", "cultivar", "startMonth", "endMonth", "method"]
      },
      "HarvestWindow": {
        "description": "Zone-specific harvest timing",
        "attributes": ["zone", "cultivar", "startMonth", "endMonth", "peakMonths"]
      },
      "DataSource": {
        "description": "Origin of timing data for validation",
        "attributes": ["type", "name", "confidence", "lastUpdated"]
      }
    },

    "relationshipTypes": {
      "HAS_ZONE": "State → Zone (one state has multiple zones)",
      "INCLUDES_REGION": "Zone → Region (zones contain geographic regions)",
      "LOCATED_IN": "Farm → Region (farm is located in region)",
      "IN_ZONE": "Farm → Zone (farm operates in specific zone)",
      "GROWS": "Farm → Cultivar (farm grows specific cultivar)",
      "IS_VARIETY_OF": "Cultivar → Product (cultivar belongs to product type)",
      "PLANTED_IN": "Cultivar → PlantingWindow (cultivar has zone-specific planting)",
      "HARVESTED_IN": "Cultivar → HarvestWindow (cultivar has zone-specific harvest)",
      "VALIDATES": "DataSource → (Planting|Harvest)Window (source confirms timing)",
      "TRIANGULATES_WITH": "DataSource ↔ DataSource (sources agree/disagree)",
      "PREDICTS": "Zone + Cultivar → HarvestWindow (GDD model prediction)"
    }
  },

